{
  "coc.preferences.jumpCommand": "drop",
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    },
    "asm-lsp": {
      "command": "asm-lsp",
      "filetypes": [
        "asm", "s", "S"
      ]
    },
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": ["cs"],
      "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
    }
  }
}
